<CustomLabel@Label>:
    size_hint: None, None
    color:(0,0,0,1)
    background_color:(1,1,1,1)
    text_size: self.size
    halign: 'center'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: self.background_color
        Rectangle:
            size: self.size
            pos: self.pos

<Fields@Label>:
    size_hint: None, None
    size: dp(50), dp(50)
    color:(0,0,0,1)
    background_color: (1,1,1,1)
    canvas.before:
        Color:
            rgba: self.background_color
        Rectangle:
            size: self.size
            pos: self.pos

<TextInputs@TextInput>:
    text: "Input field..."
    multiline: False
    input_filter: 'int'
    size_hint: None, None
    size: dp(157), dp(30)

<ActionButtons@Button>:
    disabled: True
    size_hint: None, None
    size: dp(100), dp(30)

<ThiefButtons@Button>:
    disabled: True
    size_hint: None, None
    size: dp(0), dp(0)
    pos: dp(0), dp(0)
    opacity: 0

<ThiefBoxes@Image>:
    source: ''
    size_hint: None, None
    allow_stretch: True
    size: dp(0), dp(0)
    pos: dp(0), dp(0)
    keep_ratio: False
    opacity: 0

<BrainButtons@ToggleButton>:
    size_hint: None, None
    size: dp(0), dp(0)
    pos: dp(0), dp(0)
    opacity: 0
    disabled: True
    background_normal: ''
    canvas.after:
        Color:
            rgba: self.background_color
        Rectangle:
            size: self.size
            pos: self.pos

<Progress@ProgressBar>:
    canvas:
        Color:
            rgb: 1, 1, 1
        BorderImage:
            border: (8, 8, 8, 8)
            pos: self.x, self.center_y - 8
            size: self.width, 16
            source: 'images/white-gray.png'
        BorderImage:
            id: brd
            border: [int(min(self.width * (self.value / float(self.max)) if self.max else 0, 8))] * 4
            pos: self.x, self.center_y - 8
            size: self.width * (self.value / float(self.max)) if self.max else 0, 16
            source: 'images/red.png' if self.value < 100 else 'images/green.png'

